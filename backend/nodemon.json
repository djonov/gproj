{
    "restartable": "rs",
    "ignore": [
        ".gitignore",
        ".eslintrc.json",
        "node_modules/",
        "build/",
        "tests/"
    ],
    "watch": [
        "src/"
    ],
    "execMap": {
        "ts": "node -r ts-node/register"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}